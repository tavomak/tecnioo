mixin validacionesPendientesCards(id, ticketText, badget1, badget2, badget3, direccion, statustext, ptos, hrs, asing, statusGlobal, statusColor)
    li.mt-2.pt-2.bg-white
        a(href="#")
            ul.tecnioo-card.d-flex.align-items-stretch.w-100
                li.tecnioo-card--flag
                li.tecnioo-card--item.px-3
                    p.tecnioo-card--title
                        i.fas.fa-phone.mr-2
                        span.tecnioo-card--title__id= id
                    p.tecnioo-card--ticket
                        i.fas.fa-layer-group.pr-2
                        small.tecnioo-card--ticket__text= ticketText
                    .tecnioo-card--content.mb-1
                        span.badge.badge-light.p-2.mr-1= badget1
                        span.badge.badge-light.p-2.mr-1= badget2
                        span.badge.badge-light.p-2.mr-1= badget3
                    p.tecnioo-card--place.pb-2
                        i.fas.fa-map-marker-alt.tc-icon 
                        small.pl-2= direccion
                li.tecnioo-card--item.px-3.ml-auto
                    .tecnioo-card--label.mb-3
                        small= statustext
                        span.badge.badge-primary.p-2.ml-2= ptos
                    p.tecnioo-card--deadline 
                        b=hrs
                    p
                        i.fas.fa-user-circle.pr-2 
                        span= asing

        .container-fluid.pb-2
            .row.justify-content-between
                .col
                    h6 
                        span.tecnioo-card--title__status(class='bg-'+statusColor )
                        =statusGlobal
                if statusColor != 'success'
                    .col.text-right
                        a.btn.btn-secondary.btn-sm.mr-auto(href="") Resolver
mixin listStado(color, name, number, button)
    a(href="#")
        ul.d-flex.justify-content-between.align-items-center
            li 
                p.m-0
                    span.tecnioo-card--title__status(class="bg-"+color)
                    =name
            li 
                p.m-0=number
            if button
                li Consultar
mixin listName(name, number)
    a(href="#")
        ul.d-flex.justify-content-between.align-items-center
            li 
                p.m-0
                    =name
            li 
                p.m-0=number
mixin tableRow(color, cierre, asingacion, sla)
    tr 
        td 
            span.tecnioo-card--title__status(class="bg-"+color)
            i.fas.fa-phone.mr-2
            span 26/01/2020
        td #12345789
        td 
            b Urgencia Cooler - Modelo - Logo
        td 0
        td
            b(class="text-color-"+color)=cierre
        td=asingacion
        td
            b=sla
        td Nombre Sucursal - Cliente Spot
        td Providencia
